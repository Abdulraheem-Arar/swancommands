{
  "name": "swancommands",
  "displayName": "swanCommands",
  "description": "this is an extension built to allow you to run swan commands in the command palette",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onLanguage:swift"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swancommands.menu",
        "title": "analysis options"
      },
      {
        "command": "swancommands.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "swancommands.callGraph",
        "title": "callGraph"
      },
      {
        "command": "swancommands.taintAnalysis",
        "title": "taintAnalysis"
      },
      {
        "command": "swancommands.typeStateAnalysis",
        "title": "typeStateAnalysis"
      },{
        "command": "swancommands.Detailed Logs",
        "title": "Show Detailed Logs"
    },{
      "command": "swancommands.cryptoAnalysis",
      "title": "cryptoAnalysis"
    },{
      "command": "swancommands.debug",
      "title": "debug"
    },{
      "command": "swancommands.setAnalysisSpec",
      "title": "set analysis spec"
    },{
      "command": "swancommands.removeError",
      "title": "Remove Error"
    },{
      "command": " swancommands.clearErrors",
      "title": "Remove Errors "
    }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swanActivityContainer",
          "title": "Swan",
          "icon": "resources/SWAN Logo HIRES Transparent.png"
        }
      ]
    },
    "views": {
      "swanActivityContainer": [
        {
          "id": "runAnalysisView",
          "name": "Run Analysis"
      },
      {
          "id": "viewResultsView",
          "name": "View Results"
      },
      {
          "id": "settingsView",
          "name": "Settings"
      },
      {
          "id": "errorsView",
          "name": "Errors"
      }
      ]
    },"menus": {
        "view/item/context": [
            {
                "command": "swancommands.removeError",
                "when": "view == errorsView && resource == errorItem",
                "group": "inline"
            }
        ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
